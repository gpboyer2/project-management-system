{
  "name": "seasaver_server",
  "version": "1.0.0",
  "main": "server.js",
  "bin": "server.js",
  "pkg": {
    "scripts": [
      "server.js",
      "database/**/*.js",
      "middleware/**/*.js",
      "routes/**/*.js",
      "services/**/*.js",
      "socket/**/*.js",
      "utils/**/*.js",
      "mvc/**/*.js",
      "enums/**/*.js",
      "plugins/**/*.js",
      "scripts/**/*.js",
      "etc/*.js",
      "work-arrangements/**/*.js"
    ],
    "assets": [
      "database/*.db",
      "database/*.sqlite",
      "database/*.sqlite3",
      "config/**/*",
      "etc/**/*",
      "static/**/*",
      "data/**/*",
      "logs/**/*",
      "init.sql",
      "*.json",
      "*.pem",
      "*.key",
      "*.crt",
      "*.pfx",
      "*.env*",
      "node_modules/better-sqlite3/lib/binding/*.node",
      "node_modules/sqlite3/lib/binding/**/*.node",
      "node_modules/bcryptjs/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-macos-x64",
      "node18-linux-x64"
    ],
    "outputPath": "dist",
    "options": [
      "experimental-modules"
    ]
  }
}